{
        "manifest_version": 2,
    
        "name": "COVID-19 Tracker",
        "short_name": "Web Time Tracker",
        "version": "1.0.0",

        "description": "Track and limit time your activity in the browser every day.",

    
        "icons": {
            "16": "icons/16x16.png",
            "32": "icons/32x32.png",
            "64": "icons/64x64.png",
            "128": "icons/128x128.png"
        },
        
        "background": {
            "scripts": ["axios.min.js","getData.js"]
        },
        "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" ,
        "permissions": [
            "tabs",
            "activeTab",
            "storage",
            "idle",
            "chrome://favicon/*",
            "webNavigation",
            "unlimitedStorage"
        ],
        "offline_enabled": true,

        "browser_action": {
            "default_popup": "index.html",
            "default_title": "COVID-19 Tracker",
            "default_icon": "icons/64x64.png"
        }
}